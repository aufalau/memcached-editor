{
  "name": "memcached-viewer",
  "displayName": "Memcached Viewer",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "memcached-view",
          "title": "Memcached Viewer",
          "icon": "resources/icon.png"
        }
      ]
    },
    "views": {
      "memcached-view": [
        {
          "id": "memcached-list",
          "name": "memcached-view",
          "contextualTitle": "Memcached Server List"
        }
      ]
    },
    "commands": [
      {
        "command": "memcached.list.refresh",
        "title": "Refresh Servers List",
        "icon": "$(refresh)",
        "category": "Memcached"
      },
      {
        "command": "memcached.list.add",
        "title": "Add Server",
        "icon": "$(add)",
        "category": "Memcached"
      },{
        "command": "memcached.server.reload",
        "title": "Reload Server",
        "icon": "$(refresh)",
        "category": "Memcached"
      },{
        "command": "memcached.server.edit",
        "title": "Edit Settings",
        "icon": "$(edit)",
        "category": "Memcached"
      },{
        "command": "memcached.server.delete",
        "title": "Delete Server",
        "icon": "$(trashcan)",
        "category": "Memcached"
      },{
        "command": "memcached.slab.filter",
        "title": "Filter Slab",
        "icon": "$(filter)",
        "category": "Memcached"
      },{
        "command": "memcached.slab.reload",
        "title": "Reload Slab",
        "icon": "$(refresh)",
        "category": "Memcached"
      },{
        "command": "memcached.key.operator",
        "title":"Open Item"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "memcached.list.add",
          "when": "view == memcached-list",
          "group": "navigation"
        },
        {
          "command": "memcached.list.refresh",
          "when": "view == memcached-list",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "memcached.server.reload",
          "when": "viewItem == server",
          "group": "inline"
        },
        {
          "command": "memcached.server.edit",
          "when": "viewItem == server",
          "group": "inline"
        },
        {
          "command": "memcached.server.delete",
          "when": "viewItem == server",
          "group": "inline"
        },
        {
          "command": "memcached.slab.filter",
          "when": "viewItem == slab",
          "group": "inline"
        },
        {
          "command": "memcached.slab.reload",
          "when": "viewItem == slab",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/memcached": "^2.2.2",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  },
  "dependencies": {
    "memcached": "^2.2.2"
  }
}
